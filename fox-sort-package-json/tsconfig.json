{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "build",
    "target": "ES2019",
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "node"
    ],
  },
  "exclude": [
    "test"
  ]
}
